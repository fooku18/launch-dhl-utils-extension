{
  "displayName": "DHL Tracking Tools",
  "name": "util-functions-dhl",
  "platform": "web",
  "version": "1.0.27",
  "description": "DHL Utils provide a variety of useful events, conditions and actions for our click and event driven tracking approach.",
  "iconPath": "./src/resources/icon/dhl.svg",
  "author": {
    "name": "DHL CSI",
    "url": "https://github.com/fooku18/launch-dhl-utils-extension"
  },
  "viewBasePath": "src/view/",
  "actions": [
    {
      "displayName": "Direct Call",
      "name": "direct-call",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "dc": {
            "type": "string",
            "pattern": "[\\w\\d]+"
          }
        }
      },
      "libPath": "src/lib/actions/directCall.js",
      "viewPath": "actions/directCall.html"
    },
    {
      "displayName": "Decibel Force Collection",
      "name": "decibel-force-collection",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/actions/decibelForceCollection.js"
    },
    {
      "displayName": "Trigger Custom Event",
      "name": "trigger-custom-event",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/actions/triggerCustomEvent.js",
      "viewPath": "actions/triggerCustomEvent.html"
    },
    {
      "displayName": "Set DataElement",
      "name": "set-dataelement",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/actions/setDataelement.js",
      "viewPath": "actions/setDataelement.html"
    }
  ],
  "events": [
    {
      "displayName": "Dynamic Event Listener",
      "name": "datalayer-event",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "path": {
            "type": "string",
            "minLength": 1
          }
        }
      },
      "libPath": "src/lib/events/datalayerEvent.js",
      "viewPath": "events/datalayerEvent.html"
    },
    {
      "displayName": "Usabilla Campaign End",
      "name": "usabilla-campaign-end",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/events/usabillaCampaignEnd.js"
    }
  ],
  "conditions": [
    {
      "displayName": "Dynamic Event Name",
      "name": "dynamic-event-name",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "condition": {
            "type": "string",
            "minLength": 1
          }
        }
      },
      "libPath": "src/lib/conditions/dynamicEventName.js",
      "viewPath": "conditions/dynamicEventName.html"
    },
    {
      "displayName": "Data-Tracking Value Event Appender",
      "name": "data-tracking-value-event-appender",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/conditions/dataTrackingValueEventAppender.js"
    }
  ],
  "sharedModules": [
    {
      "name": "tracker-setter",
      "libPath": "src/lib/sharedModules/trackerSetter.js"
    }
  ],
  "dataElements": [
    {
      "displayName": "Previous Value",
      "name": "previous-value",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "details": {
            "type": "string",
            "minLength": 1
          }
        }
      },
      "libPath": "src/lib/dataElements/previousValue.js",
      "viewPath": "dataElements/previousValue.html"
    },
    {
      "displayName": "Previous Page Seconds Spent",
      "name": "previous-page-seconds-spent",
