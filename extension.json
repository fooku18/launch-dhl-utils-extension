        "properties": {}
      },
      "libPath": "src/lib/dataElements/clientDayType.js"
    },
    {
      "displayName": "Page Load Time",
      "name": "page-load-time",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/dataElements/pageLoadTime.js"
    },
    {
      "displayName": "Seconds Since Pageload",
      "name": "seconds-since-pageload",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/dataElements/secondsSincePageload.js"
    },
    {
      "displayName": "Previous Page Percentage Viewed",
      "name": "previous-page-percentage-viewed",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/dataElements/previousPagePercentageViewed.js",
      "viewPath": "dataElements/previousPagePercentageViewed.html"
    },
    {
      "displayName": "URL Pieces",
      "name": "url-pieces",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/dataElements/urlPieces.js",
      "viewPath": "dataElements/urlPieces.html"
    },
    {
      "displayName": "Traffic Type",
      "name": "traffic-type",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {}
      },
      "libPath": "src/lib/dataElements/trafficType.js"
    }
  ]
}
